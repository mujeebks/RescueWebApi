<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Development.Dal.Common.Models.IncidentsDao,Development.Dal" table="Incidents" lazy="true">
    <id name="Id" column="ID" type="int">
      <generator class="native"/>
    </id>
    <property type="string"  name="Lat" column="Lat"/>
    <property type="string" name="Lang" column="Lang"/>
    <property type="int"  name="ReporterID" column="ReporterID"/>
    <property type="bool" name="VictimCategory" column="VictimCategory"/>
    <property type="DateTime"  name="DateCreated" column="DateCreated"/>
    <property type="string"  name="Summary" column="Summary"/>
    <property type="bool"  name="IsRescued" column="IsRescued"/>
    <property type="int"  name="PriorityLevel" column="PriorityLevel"/>
  </class>
</hibernate-mapping>
